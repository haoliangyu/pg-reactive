{
  "name": "pg-reactive",
  "version": "0.3.4",
  "description": "RxJS 5 interface for PostgreSQL",
  "main": "dist/pg-reactive.js",
  "types": "typescript/pg-reactive.d.ts",
  "scripts": {
    "test": "npm install --no-save rxjs@5 && node test/preparation/setup.js && mocha test/**/*.test.js --require babel-register",
    "build": "babel src --out-dir dist",
    "prepush": "npm test",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haoliangyu/pg-reactive.git"
  },
  "keywords": [
    "postgresql",
    "rxjs5",
    "rx",
    "observable",
    "reactive-x"
  ],
  "author": "Haoliang Yu <haholiang.yu@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haoliangyu/pg-reactive/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-template": "^6.26.0",
    "babel-types": "^6.26.0",
    "chai": "^4.1.2",
    "config": "^1.30.0",
    "eslint": "^4.19.1",
    "eslint-plugin-json": "^1.2.0",
    "husky": "^0.14.3",
    "mocha": "^5.2.0"
  },
  "peerDepedencies": {
    "rxjs": ">=5"
  },
  "dependencies": {
    "is-observable": "^1.1.0",
    "pg": "^7.4.3",
    "pg-query-stream": "^1.1.1"
  }
}
