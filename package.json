{
  "name": "pg-reactive",
  "version": "1.0.0",
  "description": "RxJS interface for PostgreSQL",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "npm install --no-save rxjs@6 && ts-node test/preparation/setup.ts && mocha --require ts-node/register test/index.test.ts",
    "build": "tsc",
    "prepush": "npm test",
    "lint": "tslint src/**/*.ts",
    "precommit": "npm run lint && npm run build && npm test && lint-staged",
    "doc": "typedoc --theme minimal --name 'pg-reactive' --out docs --readme README.md --includeDeclarations --excludeExternals src"
  },
  "lint-staged": {
    "src/*.{d.ts,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haoliangyu/pg-reactive.git"
  },
  "keywords": [
    "postgresql",
    "rxjs6",
    "rx",
    "observable",
    "reactive-x"
  ],
  "author": "Haoliang Yu <haholiang.yu@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haoliangyu/pg-reactive/issues"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/config": "0.0.34",
    "@types/mocha": "^5.2.0",
    "chai": "^4.1.2",
    "config": "^1.30.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "mocha": "^5.2.0",
    "prettier": "^1.13.0",
    "ts-node": "^6.0.5",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3"
  },
  "peerDepedencies": {
    "rxjs": ">=6"
  },
  "dependencies": {
    "@types/pg": "^7.4.9",
    "@types/pg-query-stream": "^1.0.1",
    "pg": "^7.4.3",
    "pg-query-stream": "^1.1.1"
  }
}
